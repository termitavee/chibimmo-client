{
  "name": "chibimmo",
  "productName": "chibimmo",
  "version": "1.0.0",
  "description": "Online multiplayer pixel game",
  "main": "src/index.js",
  "scripts": {
    "start": "electron-forge start",
    "pack": "electron-forge package",
    "build": "electron-forge make",
    "db": "sudo service mongod start",
    "server": "pm2 start ../chibimmo-server/index.js --name chibimmo",
    "lint": "eslint src"
  },
  "keywords": [],
  "author": "termitavee",
  "license": "MIT",
  "config": {
    "forge": {
      "make_targets": {
        "win32": [
          "zip",
          "squirrel",
          "appx"
        ],
        "darwin": [
          "zip",
          "dmg"
        ],
        "linux": [
          "zip",
          "deb",
          "rpm"
        ]
      },
      "electronPackagerConfig": {
        "dir": "./src",
        "packageManager": "npm",
        "asar": false,
        "icon": "./src/img/icon.jpg"
      },
      "electronWinstallerConfig": {
        "name": "chibimmon"
      },
      "electronInstallerDebian": {},
      "electronInstallerRedhat": {},
      "github_repository": {
        "owner": "",
        "name": ""
      },
      "windowsStoreConfig": {
        "packageName": "",
        "name": "chibimmon"
      }
    }
  },
  "dependencies": {
    "electron-compile": "^6.4.2",
    "electron-devtools-installer": "^2.2.1",
    "electron-is-dev": "^0.3.0",
    "lowdb": "^1.0.0",
    "phaser-ce": "^2.9.2",
    "purecss": "^1.0.0",
    "socket.io": "^2.0.4",
    "svg-captcha": "^1.3.11",
    "vue": "^2.4.4",
    "vue-router": "^3.0.1",
    "vueify": "^9.4.1"
  },
  "devDependencies": {
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "electron-forge": "^4.1.4",
    "electron-prebuilt-compile": "1.7.6",
    "eslint": "^3",
    "eslint-config-airbnb": "^15",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^5",
    "eslint-plugin-react": "^7.5.1",
    "webpack": "^3.9.1",
    "vueify": "^9.4.1"

  }
}
